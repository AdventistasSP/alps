{{# article_nav }}
&lt;nav class=&quot;article-nav theme--secondary-background-color&quot;&gt;
  &lt;div class=&quot;article-nav__inner layout-container&quot;&gt;
    &lt;div class=&quot;dropdown&quot;&gt;
      &lt;div class=&quot;dropdown__label font--secondary--s upper js-toggle-parent white&quot;&gt;News Menu &lt;span class=&quot;dropdown__arrow dib arrow--down border-top--white va--middle&quot;&gt;&lt;/span&gt;&lt;/div&gt;
      &lt;ul class=&quot;article-nav__list dropdown__options theme--secondary-background-color&quot;&gt;
        {{# items }}
          &lt;li class=&quot;article-nav__list-item dropdown__item&quot;&gt;&lt;a class=&quot;article-nav__link dropdown__item-link white&quot; href=&quot;{{ url }}&quot;&gt;{{{ text }}}&lt;/a&gt;&lt;/li&gt;
        {{/ items }}
      &lt;/ul&gt; &lt;!-- /.article-nav__list --&gt;
    &lt;/div&gt;
    &lt;div class=&quot;article-nav__search cf&quot;&gt;{{&gt; molecules-search }}&lt;/div&gt;
  &lt;/div&gt;
&lt;/nav&gt; &lt;!-- /.article-nav --&gt;
{{/ article_nav }}
