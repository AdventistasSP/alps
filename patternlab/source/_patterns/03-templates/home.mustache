    {{> organisms-hero-carousel }}

    <div class="layout-container">
      <div class="cf bg--white">
        <div class="shift-left--fluid column__primary">
          {{> organisms-body-content }}
        </div> <!-- /.shift-left--fluid -->
        <div class="shift-right--fluid column__secondary bg--beige">
