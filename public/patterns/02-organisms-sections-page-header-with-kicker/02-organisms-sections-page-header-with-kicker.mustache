{{# page_header }}
{{# has_image }}
  &lt;style type=&quot;text/css&quot;&gt;
    .header-swath--with-image {
      background-image: url({{{ img_url_s }}});
    }
    @media (min-width: 900px) {
      .header-swath--with-image {
        background-image: url({{{ img_url_m }}});
      }
    }
    @media (min-width: 1100px) {
      .header-swath--with-image {
        background-image: url({{{ img_url_l }}});
      }
    }
  &lt;/style&gt;
{{/ has_image }}
&lt;header class=&quot;header__swath theme--primary-background-color {{ page_header_class }}&quot;&gt;
  &lt;div class=&quot;layout-container cf&quot;&gt;
    {{# header_text }}
      &lt;div class=&quot;header__text&quot;&gt;
        &lt;div class=&quot;unify show-at--small&quot;&gt;
          &lt;h2 class=&quot;font--secondary--l upper white&quot;&gt;{{ heading }}&lt;/h2&gt;
          &lt;h3 class=&quot;font--secondary--m white--trans&quot;&gt;{{ subhead }}&lt;/h3&gt;
        &lt;/div&gt;
        &lt;div class=&quot;header__logo&quot;&gt;{{&gt; atoms-image }}&lt;/div&gt;
      &lt;/div&gt; &lt;!-- /.header__text --&gt;
    {{/ header_text }}
    &lt;div class=&quot;flex-container cf&quot;&gt;
      &lt;div class=&quot;shift-left--fluid&quot;&gt;
        {{# kicker }}&lt;span class=&quot;kicker white&quot;&gt;{{{ kicker }}}&lt;/span&gt;{{/ kicker }}
        &lt;h1 class=&quot;font--tertiary--xl white&quot;&gt;{{ heading }}&lt;/h1&gt;
      &lt;/div&gt;
      &lt;div class=&quot;shift-right--fluid&quot;&gt;&lt;/div&gt; &lt;!-- /.shift-right--fluid --&gt;
    &lt;/div&gt;
  &lt;/div&gt;
&lt;/header&gt; &lt;!-- /.header__swath --&gt;
{{/ page_header }}
